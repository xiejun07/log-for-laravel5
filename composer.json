{
  "name": "xiejun/laravel-log-viewer",
  "description": "A Laravel log reader",
  "license": "MIT",
  "keywords": ["log", "log-reader", "log-viewer", "logging", "laravel", "lumen"],
  "authors": [
    {
      "name": "xiejun",
      "email": "xiejun941107@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "4.2.*|5.*"
  },
  "autoload": {
    "classmap": [
      "src/controllers"
    ],
    "psr-4": {
      "Rap2hpoutre\\LaravelLogViewer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rap2hpoutre\\LaravelLogViewer\\LaravelLogViewerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
